{
	"name": "rnuix",
	"version": "1.2.0",
	"main": "./src/index.js",
	"bin": "./bin/rnuix",
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"fmt": "prettier --write --print-width 80 --tab-width 4 --single-quote --trailing-comma all --bracket-spacing --parser flow '!(node_modules|bin|flow-typed|coverage)/**/*.js' '*.js'",
		"test": "npm run flow && npm run jest",
		"flow": "flow --show-all-errors",
		"jest": "jest --coverage",
		"jest:watch": "jest --watch",
		"precommit": "npm run fmt",
		"prepush": "npm run test"
	},
	"dependencies": {
		"chokidar": "1.7.0",
		"commander": "2.11.0",
		"denodeify": "1.2.1",
		"glob": "7.1.2",
		"react-navigation": "1.0.0-beta.11"
	},
	"peerDependencies": {
		"react": ">=15.4.0-rc.4 <= 16.0.0",
		"react-native": ">=0.39.0 <= 0.48"
	},
	"devDependencies": {
		"babel-jest": "20.0.3",
		"babel-plugin-transform-flow-strip-types": "6.22.0",
		"babel-preset-react-native": "2.1.0",
		"flow-bin": "0.49.1",
		"husky": "0.14.3",
		"jest": "20.0.4",
		"prettier": "1.5.3",
		"react": "16.0.0-alpha.12",
		"react-native": "0.47.2",
		"react-test-renderer": "16.0.0-alpha.12"
	},
	"jest": {
		"preset": "react-native",
		"coveragePathIgnorePatterns": [
			"/node_modules/",
			"demo.js"
		],
		"transformIgnorePatterns": [
			"<rootDir>/node_modules/(?!react-native|react-native-*|react-clone-referenced-element|react-navigation)"
		]
	}
}
